func factorial(value)
    if value <= 1 then
        return 1
    end
    return value * factorial(value - 1)
end

var message = "Hello "

message += "World!"

if false then
    for var i = 0, i < strlen(message), i += 1 do
        print(message[i])
        if i >= 9 then
            break
        end
    end
end

if true then
    var i = 0
    while i < strlen(message) do
        print(message[i])
        i += 1
    end
end
